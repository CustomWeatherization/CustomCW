    function diffDays() {    
        var ndays;
        var date1 = new Date(document.getElementById('<%=txtFromDate.ClientID%>').value);
        var date2 = new Date(document.getElementById('<%=txtToDate.ClientID%>').value);
        var lblFromTime = document.getElementById('<%=lblFromTime.ClientID%>');
        var lblToTime = document.getElementById('<%=lblToTime.ClientID%>');
        var ddlFromTime = document.getElementById('<%=ddlFromTime.ClientID%>');
        var ddlToTime = document.getElementById('<%=ddlToTime.ClientID%>');
        var FromTime = document.getElementById('<%=RequiredFieldValidator4.ClientID%>');
        var ToTime = document.getElementById('<%=RequiredFieldValidator5.ClientID%>');
     
          var ddlFromTimeText = ddlFromTime.options[ddlFromTime.selectedIndex].text;
         var ddlToTimeText = ddlToTime.options[ddlToTime.selectedIndex].text;
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1;
        var yyyy = today.getFullYear();
        var currentdate =new Date(mm + "/" + dd + "/" + yyyy);
        var days = (currentdate - date1) / 1000 / 60 / 60 / 24;
        var ddlReport = document.getElementById("<%=ddlTypeOfRequested.ClientID%>");
        var ddlSelectedText = ddlReport.options[ddlReport.selectedIndex].text;
        if ((ddlSelectedText != "Sickness" && ddlSelectedText != "Unpaid Sickness") && days <= 0) {
            if (date1.getTime() == date2.getTime()) {
                document.getElementById('<%=lblFromTime.ClientID%>').style.display = 'block';
                document.getElementById('<%=lblToTime.ClientID%>').style.display = 'block';
                document.getElementById('<%=ddlFromTime.ClientID%>').style.display = 'block';
